  "version": "0.2.0",
  "configurations": [
		{
			"name": "Launch executable",
			"type": "go",
			"request": "launch",
			"mode": "exec",
			"program": "${workspaceFolder}/bin/serverless-installer"
		},
		{
			"name": "Launch file",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${file}"
		},
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "env": {},
      "args": []
		}		
  ]
